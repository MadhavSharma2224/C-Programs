#include <stdio.h>
void swap(int *a,int *b);
int main()
{  
    int m=10,n=20;
    printf("%d\n",m);
    printf("%d\n",n);
    swap(&m,&n);
    printf("After swapping %d,%d",m,n);
    return 0;
}

void swap(int *a,int *b)
{
    int temp;
    temp=*a;
    *a=*b;
    *b=temp;
}
